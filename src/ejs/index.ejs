<!doctype html>
<html lang="en">

<% const title="VentureBuddy" %>
  <% const tagline="Grow Your Enterprise Business with the help of The Crowd" %>
    <% const url="https://venturebuddy.net" %>
      <% const
        description="We are building the technology infrastructure for the future of Business Development. We help ventures & new market entrants grow, by maximizing Business Development Opportunities."
        %>
        <% const image=url+"/images/card-image-en.jpg" %>
          <% const twitter="@twitter_account" %>

            <head>
              <!-- Required meta tags -->
              <meta charset="utf-8">
              <meta name="viewport" content="width=device-width, initial-scale=1">
              <meta name="robots" content="index, follow">

              <!-- CSS -->
              <link href="css/style.min.css" rel="stylesheet">

              <!-- Web Fonts -->
              <link rel="preconnect" href="https://fonts.googleapis.com">
              <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
              <link
                href="https://fonts.googleapis.com/css2?family=Barlow:wght@500&family=Poppins:wght@300;400;500;600;700&display=swap"
                class="webfont">

              <title>
                <%= title %> - <%= tagline %>
              </title>
              <meta name="description" content="<%= description %>">
              <!-- <link rel="canonical" href="<%= url %>"> -->

              <!-- Open Graph Tags -->
              <meta property="og:title" content="<%= title %> - <%= tagline %>" />
              <meta property="og:type" content="website" />
              <meta property="og:image" content="<%= image %>" />
              <meta property="og:description" content="<%= description %>" />
              <meta property="og:locale" content="en-US" />
              <meta property="og:site_name" content="<%= title %>" />
              <meta property="og:url" content="<%= url %>" />

              <!-- Twitter Cards -->
              <meta name="twitter:card" content="summary_large_image">
              <meta name="twitter:site" content="<%= twitter %>">
              <meta name="twitter:title" content="<%= title %> - <%= tagline %>">
              <meta name="twitter:description" content="<%= description %>">
              <meta name="twitter:image" content="<%= image %>">
              <meta property="twitter:url" content="<%= url %>" />
              <meta name="twitter:creator" content="<%= twitter %>">

              <!-- Favicon -->
              <link rel="apple-touch-icon" sizes="180x180" href="images/favicon/apple-touch-icon.png">
              <link rel="icon" type="image/png" sizes="32x32" href="images/favicon/favicon-32x32.png">
              <link rel="icon" type="image/png" sizes="16x16" href="images/favicon/favicon-16x16.png">
              <link rel="manifest" href="images/favicon/site.webmanifest">
              <link rel="mask-icon" href="images/favicon/safari-pinned-tab.svg" color="#353bc9">
              <meta name="msapplication-TileColor" content="#ffffff">
              <meta name="theme-color" content="#ffffff">
            </head>

            <body class="en">

              <%- include('_svg-defs'); %>

                <div class="hero-bg">

                  <header class="page-header w-100">
                    <div class="page-header__inner container-xxl d-flex align-items-center justify-content-between">

                      <a class="header-logo fw-600" href="#">
                        <%= title %>
                      </a>

                      <input type="checkbox" class="navTriggerInput" id="navTrigger">
                      <label for="navTrigger" class="navTriggerToggle">
                        <div class="spinner diagonal part-1"></div>
                        <div class="spinner horizontal"></div>
                        <div class="spinner diagonal part-2"></div>
                      </label>

                      <nav class="header-nav">
                        <ul class="d-lg-flex list-unstyled p-0 m-0">
                          <li>
                            <a href="#vision" class="header-nav-item">Vision</a>
                          </li>
                          <li>
                            <a href="#services" class="header-nav-item">Services</a>
                          </li>
                          <li>
                            <a href="#member" class="header-nav-item">Member</a>
                          </li>
                          <li>
                            <a href="#core-values" class="header-nav-item">Core Values</a>
                          </li>
                          <li>
                            <a href="#company" class="header-nav-item">Company</a>
                          </li>
                          <li>
                            <a href="#contact" class="header-nav-item">Contact</a>
                          </li>
                          <li>
                            <a href="ja/" class="header-nav-item">
                              <svg class="icon">
                                <use xlink:href="#icon-globe"></use>
                              </svg>
                              日本語
                            </a>
                          </li>
                        </ul>
                      </nav>
                    </div>
                  </header>

                  <section id="hero" class="d-flex w-100 align-items-center">
                    <div class="container-xxl">
                      <div class="text-sm-center">
                        <h1 class="text-white mb-4 mb-sm-5">
                          Grow Your Enterprise Business <br><small class="fw-400 d-block d-sm-inline">with the help of
                          </small>The Crowd
                        </h1>
                        <p class="text-white">Great Ventures Reaching the right places faster</p>
                      </div>
                    </div>

                  </section>

                </div><!-- .hero-bg -->


                <section id="introduction">
                  <div class="container-xxl text-sm-center">
                    <h2 class="mb-4">We are building the technology infrastructure <br class="d-none d-sm-block">for the
                      future of Business
                      Development</h2>
                    <p>We help ventures & new market entrants grow, <br class="d-none d-sm-block">by maximizing Business
                      Development Opportunities
                    </p>
                  </div>
                </section>

                <section id="vision">
                  <div class="container-xl d-lg-flex justify-content-between">
                    <div class="vision-title">
                      <h2 class="mb-4 text-center text-lg-start">Vision</h2>
                      <h3 class="text-primary ms-sm-2">We are committed to transform Tokyo to the next Silicon
                        Valley
                      </h3>
                    </div>
                    <div class="vision-text pt-2 pt-sm-4">
                      <p>“Japan has one of the most advanced innovation ecosystems globally.. however not paired with an
                        equally robust and capable start-up ecosystem.” <br>-- the World Bank Research about Tokyo
                        Startup
                        Ecosystem in 2021</p>

                      <p>One of the outcomes due to the lack of support infrastructure is that considerable amount of
                        companies in the world hesitate to enter Japanese market. Our mission is to transform a closed
                        community of Japan to a truly transnational ecosystem by creating “international connectedness”
                        that supports new market entratns and startup’s to grow. Business Development is going to be
                        key.
                      </p>

                      <p>We will collaborate with multiple of global stakeholders and real brands to foster the
                        ecosystem
                        in japan to nurture the next innovation to come.</p>
                    </div>
                  </div>
                </section>

                <section id="services">
                  <div class="container-xl d-lg-flex justify-content-between">
                    <h2 class="text-center text-lg-start mb-5">Service</h2>
                    <ol class="list-unstyled m-0 p-0">
                      <li class="d-flex mb-4 mb-sm-5">
                        <span class="text-black fw-600 ms-4 d-block">
                          A web platform <small class="fw-300 d-block">for Business Development in Japan</small>
                        </span>
                      </li>
                      <li class="d-flex">
                        <span class="text-black fw-600 ms-4 d-block">Events</span>
                      </li>
                    </ol>
                  </div>
                </section>

                <section id="member">
                  <div class="container-lg">
                    <h2 class="text-center mb-5">Member</h2>
                    <ul class="list-unstyled p-0 m-0 member-list">

                      <% var memberData=memberDataEnJson %>
                        <% memberData.forEach(function (value) { %>
                          <li>
                            <div class="lh-0 mb-4 text-center text-sm-start">
                              <picture>
                                <source srcset="images/member-<%= value.slug %>.webp" type="image/webp">
                                <source srcset="images/member-<%= value.slug %>.jpg" type="image/jpeg">
                                <img src="images/member-<%= value.slug %>.jpg" loading="lazy" width="256" height="256"
                                  alt="<%= value.name %>'s profile picture" title="<%= value.name %>'s profile picture">
                              </picture>
                            </div>
                            <h3 class="mb-4">
                              <%= value.name %> <small class="d-block mt-1 fw-400">
                                  <%= value.title %>
                                </small>
                            </h3>
                            <p>
                              <%= value.bio %>
                            </p>
                          </li>
                          <% }); %>

                    </ul>
                  </div>
                </section>

                <section id="core-values">
                  <div class="container-xl">
                    <h2 class="text-center mb-5">Core Values</h2>
                    <ul class="list-unstyled p-0 m-0 core-values-list">

                      <% var valData=valDataJson %>
                        <% valData.forEach(function (value) { %>
                          <li class="px-4 py-5">
                            <div class="lh-0 text-center">
                              <picture>
                                <source srcset="images/val-<%= value.slug %>.webp" type="image/webp">
                                <source srcset="images/val-<%= value.slug %>.png" type="image/png">
                                <img src="images/val-<%= value.slug %>.png" loading="lazy" width="80" height="80"
                                  alt="<%= value.title %> icon" title="<%= value.title %> icon">
                              </picture>
                            </div>
                            <h3 class="text-center text-primary mt-4 mb-3">
                              <%= value.title %>
                            </h3>
                            <p>
                              <%= value.description %>
                            </p>
                          </li>
                          <% }); %>

                    </ul>
                  </div>
                </section>

                <section id="company">
                  <div class="container-xl d-lg-flex justify-content-between">
                    <h2 class="text-center text-lg-start mb-5">Company</h2>
                    <table class="table ms-lg-5 border-top">
                      <tr>
                        <th>Company Name</th>
                        <td>ReachNext Tokyo Inc.</td>
                      </tr>
                      <tr>
                        <th>Establishment</th>
                        <td>2022</td>
                      </tr>
                      <tr>
                        <th>Services</th>
                        <td>
                          <ul class="m-0">
                            <li>VentureBuddy - A web platform for Business Development in Japan</li>
                            <li>Events</li>
                          </ul>
                        </td>
                      </tr>
                      <tr>
                        <th>CEO</th>
                        <td>Toji Morimoto</td>
                      </tr>
                      <tr>
                        <th>Location</th>
                        <td>Tokyo, Japan</td>
                      </tr>
                    </table>
                  </div>
                </section>

                <section id="contact" class="d-flex justify-content-center container">
                  <div class="contact-bg">
                    <div class="contact-inner text-center">
                      <h2>Contact Us</h2>
                      <p class="text-white my-4">Please feel free to contact us <br>if you need any further information
                      </p>
                      <div class="contact-email-address pb-2">
                        <img src="images/email-address.svg" loading="lazy" width="474" height="36" alt="Email Address"
                          title="Email Address">
                      </div>
                    </div>
                  </div>
                </section>

                <footer class="text-center page-footer pb-5">
                  © 2022 <%= title %>. All Rights Reserved.
                </footer>

                <script>
                  // WebFont lazy load
                  const webfonts = document.querySelectorAll(".webfont");
                  webfonts.forEach(function (webfont) {
                    webfont.rel = "stylesheet";
                  });
                </script>

                <script>
                  // Sticky header background
                  window.addEventListener("scroll", function () {
                    const header = document.querySelector(".page-header");
                    header.classList.toggle("scroll", window.scrollY > 100);
                  });
                </script>

                <script>
                  const navTrigger = document.querySelector("#navTrigger");
                  const navItems = document.querySelectorAll(".header-nav-item")

                  for (const item of navItems) {
                    item.addEventListener('click', function (event) {
                      console.log('click');
                      if (navTrigger.checked == true) {
                        navTrigger.checked = false;
                      }
                    })
                  }
                </script>

                <script src="js/modernizr-webp.js"></script>
            </body>

</html>
